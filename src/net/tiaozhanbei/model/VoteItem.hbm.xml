<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="net.tiaozhanbei.model">
  <class catalog="TZCup" name="net.tiaozhanbei.model.VoteItem" schema="dbo" table="voteitem">
    <id name="id" type="integer">
      <column name="voteitem_id"/>
      <generator class="identity"/>
    </id>
    <property generated="never" lazy="false" name="content" type="string"/>
    <property generated="never" lazy="false" name="votes" type="integer"/>
  </class>
</hibernate-mapping>
